<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="symfony_cmf_blog.blog_route_manager" class="Symfony\Cmf\Bundle\BlogBundle\Routing\BlogRouteManager">
            <argument type="service" id="doctrine_phpcr.odm.default_document_manager"/>
            <argument>%symfony_cmf_blog.routing_basepath%</argument>
            <argument>%symfony_cmf_blog.routing_post_controller%</argument>
            <argument>%symfony_cmf_blog.routing_post_prefix%</argument>
        </service>

    </services>

</container>


